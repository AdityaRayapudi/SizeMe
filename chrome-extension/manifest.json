{
    "manifest_version": 3,
    "name": "FitMe",
    "version": "1.0",
    "description": "Adds a button next to the 'Size' section on webpages.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_title": "FitMe"
    }
  }